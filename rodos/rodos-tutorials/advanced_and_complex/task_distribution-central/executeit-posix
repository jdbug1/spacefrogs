#   !/usr/bin/sh

posix-executable gateway.cpp globals.cpp taskmanager.cpp usertask_evencnt.cpp usertask_negcnt.cpp usertask_oddcnt.cpp usertask_primcnt.cpp
mv tst tst_usernode

posix-executable gateway.cpp globals.cpp taskdistributor.cpp 
mv tst tst_masternode


xterm -bg white  -fg black -title usernode -e tst_usernode &
xterm -bg white  -fg black -title usernode -e tst_usernode &
xterm -bg white  -fg black -title usernode -e tst_usernode &

sleep 2
xterm -bg yellow  -fg black -title masternode -e tst_masternode &


